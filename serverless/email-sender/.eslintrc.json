{
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 2017
  },
  "env": {
      "es6": true,
      "node": true
  },
  "rules": {
    "indent": ["error", 4],
    "no-console": "off",
    "quotes": ["error", "single", { "allowTemplateLiterals": true }],
    "no-unused-vars": "off"
  }
}

